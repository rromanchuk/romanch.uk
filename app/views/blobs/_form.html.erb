<%= simple_form_for blob, html: { data: { turbo_frame: :_top} } do |f| %>
  <%= f.error_notification %>
  <%= f.input :key %>
  <%= f.input :title %>
  <%= f.input :description, as: :text %>
  <%= f.input :slug %>
  <%= f.input :content_type %>
  <%= f.input :tags_as_string, as: :string %>
  <%= f.input :thumbnail_key, as: :string %>
  <%= f.button :submit %>
<% end %>
