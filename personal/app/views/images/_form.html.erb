<%= simple_form_for image, html: { data: { turbo_frame: :_top} } do |f| %>
  <%= f.error_notification %>
  <%= f.input :key %>
  <%= f.input :title %>
  <%= f.input :caption %>
  <%= f.input :slug %>
  <%= f.input :tags_as_string, as: :string %>
  <%= f.input :imageable_id %>
  <%= f.input :imageable_type %>
  <%= f.submit class: "btn-primary" %>
<% end %>