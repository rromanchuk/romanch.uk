<%- content_for :page_title, 'Ryan Romanchuk' %>
<% content_for :page_description, 'Full-stack developer in the consumer mobile space.' %>
<% content_for :share_image, 'https://romanch.uk/static/easter-eggs/ryan_romanchuk.jpg' %>
<% content_for :canonical_link_tag do %>
  <link rel="canonical" href="<%= root_url(host: 'romanch.uk') %>" />
<% end %>

<% content_for :json_ld do %>
  <%= render 'shared/me' %>
<% end %>


<% content_for :right_col do %>
  <%= render CategoriesComponent.new %>
<% end %>


<%= render 'landing' %>



<section class="row">
  <hr class="col-3 col-md-2 mt-5">
  <%= turbo_frame_tag :posts, src: posts_path, loading: :lazy do %>
    <section class="row d-none"> </section>
  <% end %>
</section>