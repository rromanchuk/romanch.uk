<%= simple_form_for video, html: { data: { turbo_frame: :_top} } do |f| %>
  <%= f.error_notification %>
  <%= f.input :key %>
  <%= f.input :thumbnail_key %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :slug %>
  <%= f.input :aspect %>
  <%= f.submit class: "btn-primary" %>
<% end %>